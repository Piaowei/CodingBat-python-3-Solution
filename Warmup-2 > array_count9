def array_count(list1):
    count = 0
    nine =9
    for i in list1 :
        if nine ==i:
            count = count+1
    return count

list1 = [9, 8, 9, 3, 9,9]
print(array_count(list1))

#with RANGE FUNCTIUON---------------------------------------------

def array_count(list1):
    count = 0
    nine =9
    for i in range(len(list1)) :
        if nine ==list1[i]:
            count = count+1
    return count

list1 = [1, 9, 9, 3, 9,9]
print(array_count(list1))
